# Source .bashrc and .profile instead

if [ -f ~/.profile ]; then
   source ~/.profile
fi

if [ -f ~/.bashrc ]; then
   source ~/.bashrc
fi

# added by Miniconda3 installer
export PATH="/Users/michaelyang/miniconda3/bin:$PATH"

# added by Miniconda3 installer
export PATH="/Users/michaelyang/miniconda3/bin:$PATH"

# The next line updates PATH for the Google Cloud SDK.
if [ -f '/Users/michaelyang/google-cloud-sdk/path.bash.inc' ]; then source '/Users/michaelyang/google-cloud-sdk/path.bash.inc'; fi

# The next line enables shell command completion for gcloud.
if [ -f '/Users/michaelyang/google-cloud-sdk/completion.bash.inc' ]; then source '/Users/michaelyang/google-cloud-sdk/completion.bash.inc'; fi
