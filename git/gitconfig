# This file is included but gitignored for
# private git configs (git user, etc)
[include]
  path = ~/.gitprivate
[alias]
  remove = rm -r --cached .
  cm = commit -m
  redo = commit --amend
  find = ls-files | find
  a = "!git add . && echo $PWD added to git."
  all = "!git add . && echo $PWD added to git."
  stat = status
  d = diff
